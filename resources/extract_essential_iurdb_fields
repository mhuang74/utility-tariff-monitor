jq '[.[] | select(.sector == "Commercial" and .country == "USA" and ((.revisions | last).date."$date" | sub("\\.\\d+"; "") | fromdate) >= (now - 15552000)) | {utilityName: .utilityName, eiaId: .eiaId, sector: .sector, rateName: .rateName, effectiveDate: .effectiveDate."$date", lastRevision: (.revisions | last).date."$date", sourceReference: .sourceReference, sourceParent: .sourceParent}] | unique_by(.eiaId)' --
